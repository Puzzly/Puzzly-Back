<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.puzzly.api.repository.mybatis.UserMybatisRepository">
    <insert id="insertUser" parameterType="com.puzzly.api.dto.request.UserRequestDto" useGeneratedKeys="true" keyProperty="userId">
        INSERT INTO tb_users(user_id, email, password, gender)
        VALUES (1, #{user.email}, #{user.password}, false);
    </insert>
</mapper>